{
  "name": "extract-to-github",
  "version": "1.0.0",
  "description": "Generates PNG for Google Drive files (Docs, Slides, PDF, etc.) in PRs",
  "main": "dist/main.js",
  "scripts": {
    "build": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "github",
    "actions",
    "google",
    "drive",
    "docs",
    "slides",
    "pdf",
    "diff",
    "visual"
  ],
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "fs-extra": "^11.3.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^148.0.0",
    "mupdf": "^1.3.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.24.0",
    "eslint-plugin-github": "^6.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
